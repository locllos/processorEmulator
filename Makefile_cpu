processor.exe: objectsFiles/processor.o objectsFiles/stack.o
	g++ objectsFiles/processor.o objectsFiles/stack.o -o processor

objectsFiles/processor.o: mainFiles/processor.cpp
	g++ -c mainFiles/processor.cpp -o objectsFiles/processor.o
	
objectsFiles/stack.o: mainFiles/stack.cpp
	g++ -c mainFiles/stack.cpp -o objectsFiles/stack.o